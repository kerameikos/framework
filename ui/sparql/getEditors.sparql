PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX foaf: <http://xmlns.com/foaf/0.1/>
PREFIX org: <http://www.w3.org/ns/org#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

SELECT ?editor ?name ?orcid ?org ?wikidata WHERE {
  ?editor a foaf:Person ;
            skos:inScheme <http://kerameikos.org/editor/> ;
            skos:prefLabel ?name ;
  OPTIONAL {?editor org:hasMembership ?membership .
            ?membership org:organization/skos:prefLabel ?org ;
              org:organization/skos:exactMatch ?wikidata }
  OPTIONAL {?editor skos:exactMatch ?orcid FILTER contains(str(?orcid), "orcid.org")}
  
} ORDER BY ?name